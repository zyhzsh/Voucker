{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/voucher/src",
  "monorepo": true,
  "root": "apps/voucher",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/voucher/tsconfig.app.json"
  },
  "projects": {
    "store": {
      "type": "application",
      "root": "apps/store",
      "entryFile": "main",
      "sourceRoot": "apps/store/src",
      "compilerOptions": {
        "tsConfigPath": "apps/store/tsconfig.app.json"
      }
    },
    "voucher": {
      "type": "application",
      "root": "apps/voucher",
      "entryFile": "main",
      "sourceRoot": "apps/voucher/src",
      "compilerOptions": {
        "tsConfigPath": "apps/voucher/tsconfig.app.json"
      }
    },
    "location": {
      "type": "application",
      "root": "apps/location",
      "entryFile": "main",
      "sourceRoot": "apps/location/src",
      "compilerOptions": {
        "tsConfigPath": "apps/location/tsconfig.app.json"
      }
    },
    "category": {
      "type": "application",
      "root": "apps/category",
      "entryFile": "main",
      "sourceRoot": "apps/category/src",
      "compilerOptions": {
        "tsConfigPath": "apps/category/tsconfig.app.json"
      }
    },
    "common": {
      "type": "library",
      "root": "libs/common",
      "entryFile": "index",
      "sourceRoot": "libs/common/src",
      "compilerOptions": {
        "tsConfigPath": "libs/common/tsconfig.lib.json"
      }
    },
    "transaction": {
      "type": "application",
      "root": "apps/transaction",
      "entryFile": "main",
      "sourceRoot": "apps/transaction/src",
      "compilerOptions": {
        "tsConfigPath": "apps/transaction/tsconfig.app.json"
      }
    },
    "inventory": {
      "type": "application",
      "root": "apps/inventory",
      "entryFile": "main",
      "sourceRoot": "apps/inventory/src",
      "compilerOptions": {
        "tsConfigPath": "apps/inventory/tsconfig.app.json"
      }
    },
    "order": {
      "type": "application",
      "root": "apps/order",
      "entryFile": "main",
      "sourceRoot": "apps/order/src",
      "compilerOptions": {
        "tsConfigPath": "apps/order/tsconfig.app.json"
      }
    },
    "ticket": {
      "type": "application",
      "root": "apps/ticket",
      "entryFile": "main",
      "sourceRoot": "apps/ticket/src",
      "compilerOptions": {
        "tsConfigPath": "apps/ticket/tsconfig.app.json"
      }
    }
  }
}
